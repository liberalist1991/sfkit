  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w ./server ./web -i assets.json --exec 'cross-env NODE_ENV=development START_MODE=dev:server node ./server/start.dev.js'",
    "start": "npm run build & node ./server/start.js",
    "debug": "node --inspect ./server/start.dev.js",
    "build": "rm -rf ./dist && npm run build:client & npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.client.dev.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.prod.js",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
  "author": "thinkzhan",
  "license": "ISC",
